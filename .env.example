COMPOSE_PROJECT_NAME="my-project-name"


# Specify the registry where the built docker image should be stored.
REGISTRY="docker-registry.mydomain.com:5000"


# Specify a name for the docker image. This should not include any registry name.
DOCKER_IMAGE_NAME="my-application"


# Specify the environment. Either "dev" or "production"
ENVIRONMENT="dev"


# Specify the database connection details.
DB_USER=
DB_PASSWORD=
DB_NAME=
